<p id="notice"><%= notice %></p>

<h1>Friendlists</h1>

<table class="table table table-striped table table-hover able table-bordered">
<thead class="table-dark">
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Twitter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @friendlists.each do |friendlist| %>
      <tr>
        <td><%= friendlist.first_name %></td>
        <td><%= friendlist.last_name %></td>
        <td><%= friendlist.email %></td>
        <td><%= friendlist.phone %></td>
        <td><%= friendlist.twitter %></td>
        <td><%= link_to 'Show', friendlist %></td>
        <td><%= link_to 'Edit', edit_friendlist_path(friendlist) %></td>
        <td><%= link_to 'Destroy', friendlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Friendlist', new_friendlist_path ,class:"btn btn-dark"%>
